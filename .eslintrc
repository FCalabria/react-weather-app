{
  "parser": "babel-eslint",
  "plugins": [
    "react"
  ],
  "ecmaFeatures": {
    "jsx": true,
    "modules": true
  },
  "env": {
    "browser": true,
    "amd": true,
    "es6": true,
    "node": true
  },
  "rules": {
    "comma-dangle": 2,
    "quotes": [ 1, "single" ],
    "no-undef": 1,
    "global-strict": 0,
    "no-extra-semi": 1,
    "no-console": 1,
    "no-unused-vars": 2,
    "no-extra-parens": 2,
    "no-irregular-whitespace": 1,
    "no-trailing-spaces": [1, { "skipBlankLines": true }],
    "no-unreachable": 2,
    "no-alert": 1,
    "react/jsx-uses-react": 2
  }
}
